

<%#= @data %>
<div class="container">
 <h1>Parcel Machines</h1>
    <table class="table table-striped">
    <thead>
        <tr>
        <th scope="col">Name</th>
        <th scope="col">ZIP</th>
        <th scope="col">Address</th>
        </tr>
    </thead>
    <tbody>
        <% @parcel_machines.each do |pm| %>
            <div></div>
            <tr>
            <td><%= pm.name %></td>
            <td><%= pm.zip %></td>
            <td><%= pm.full_addresses %></td>
            </tr>
        <% end %>  
    </tbody>
    </table>

    <div><%= paginate @parcel_machines, theme: 'twitter-bootstrap-4',
                   pagination_class: "pagination-md justify-content-center",
                   nav_class: "d-inline-b" %></div>
</div>